<tr onclick="window.location.href = '/competitions/<%= competition.id %>'">
  <td><%= competition.title %></td>
  <td><%= competition.startdate.strftime('%m/%d/%Y %H:%M:%S') %></td>
  <td><%= competition.enddate.strftime('%m/%d/%Y %H:%M:%S') %></td>
  <td><%= competition.tracks.count %></td>
  <td><%= competition.constraints %></td>
</tr>