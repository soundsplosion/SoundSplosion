<head>
  <script src="<%= asset_path("gritter.js")%>"></script>
  <link rel="import" href="<%= asset_path("dialog.html")%>">
  <script src="http://code.highcharts.com/highcharts.js"></script>
  <script src="http://code.highcharts.com/modules/exporting.js"></script>
  <link href="<%= asset_path("gritter.css")%>">
  <link rel="stylesheet" href="//code.jquery.com/ui/1.10.3/themes/smoothness/jquery-ui.css">
  <%= javascript_include_tag "//www.google.com/jsapi", "chartkick" %>
</head>
<script>
  var denoto_rhomb = makeDenotoRhombus(); 
</script>
<body>
<div style="margin-left: auto; margin-right: auto; width: 700px">
  <table id="track-table" style="background: #172837;">
    <tbody>
        <% @tracks.order('id DESC').each do |track| %>
          <%= render "audio_player", track: track, is_entry_table: 'false' %>
        <% end %>
    </tbody>
  </table>
  <%= will_paginate @tracks %>
  <denoto-dialog id="appdialogbox"></denoto-dialog>
</div>
</body>
<script>
  <% @tracks.order('id DESC').each do |track| %>
    <%= render 'tracks/audio_player.js.erb', track: track %>
  <% end %>
</script>
<style>
#loading-img {
  position: absolute;
  left: 350px;
  top: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  width: 60px;
  height: 130px;
  padding-top: 80px;
  padding-bottom: 10px;
}
#loader {
  position: relative;
}
</style>
